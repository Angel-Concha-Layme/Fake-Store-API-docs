---
import type { Category } from '@models/index';
const API = 'https://fake-store-api-409620.rj.r.appspot.com/api/categories/all';
const response = await fetch(API);
const responseData = await response.json();
const data = responseData.content.map((item: Category) => ({
  ...item
}));
---
<div class='grid grid-5 not-content'>
	{data.map((item: Category) => (
    <article>
      <img loading="lazy"src={item.image} alt={item.name} />
      <p>{item.name}</p>
    </article>
  ))}
</div>


